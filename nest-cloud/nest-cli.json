{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/client/src",
  "compilerOptions": {
    "assets": [
      "**/*.yaml"
    ],
    "watchAssets": true,
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/client/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/client",
  "projects": {
    "client": {
      "type": "application",
      "root": "apps/client",
      "entryFile": "main",
      "sourceRoot": "apps/client/src",
      "compilerOptions": {
        "tsConfigPath": "apps/client/tsconfig.app.json"
      }
    },
    "user-service": {
      "type": "application",
      "root": "apps/user-service",
      "entryFile": "main",
      "sourceRoot": "apps/user-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-service/tsconfig.app.json"
      }
    },
    "shared": {
      "type": "library",
      "root": "libs/shared",
      "entryFile": "index",
      "sourceRoot": "libs/shared/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/tsconfig.lib.json"
      }
    },
    "portal-service": {
      "type": "application",
      "root": "apps/portal-service",
      "entryFile": "main",
      "sourceRoot": "apps/portal-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/portal-service/tsconfig.app.json"
      }
    }
  }
}